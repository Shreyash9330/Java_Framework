<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="a1" class="mypack.Answer">
		<property name="aname" value="Platform Independent"></property>
		<property name="postedby" value="Vinaya"></property>	
	</bean>
	
	<bean id="a2" class="mypack.Answer">
		<property name="aname" value="OOP"></property>
		<property name="postedby" value="Ganesh"></property>
	</bean>

	<bean id="quis" class="mypack.Question">
		<property name="qid" value="1"></property>
		<property name="qname" value="What is Java"></property>
		
		<property name="ans">
			<list>
		     		<ref bean="a1"/>
					<ref bean="a2"/>
			</list>
		</property>
		
		
	</bean>



</beans>